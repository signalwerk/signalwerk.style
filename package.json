{
  "name": "signalwerk-styles",
  "version": "1.0.0",
  "description": "signalwerk css system",
  "main": "dist/index.js",
  "scripts": {
    "build": "NODE_ENV=production npm run clean && npm run md && webpack",
    "clean": "rm -rf ./dist  && mkdir -p ./dist",
    "md": "npm run clean && showdown makehtml -i ./src/index.md -o ./dist/text.html --tables && sh build.sh && rm -rf ./dist/text.html",
    "start": "npm run md && webpack-dev-server"
  },
  "author": "Stefan Huber (Signalwekr GmbH)",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^9.1.5",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "config": "^2.0.1",
    "css-loader": "^1.0.0",
    "css-mqpacker": "^7.0.0",
    "cssnano": "^4.1.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.6.0",
    "node-sass": "^4.9.3",
    "normalize.css": "^8.0.0",
    "postcss-calc": "^7.0.1",
    "postcss-conditionals": "^2.1.0",
    "postcss-critical-split": "^2.5.3",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-nested": "^4.1.2",
    "postcss-pipeline-webpack-plugin": "^5.0.0",
    "postcss-preset-env": "^6.6.0",
    "postcss-simple-vars": "^5.0.2",
    "sass-loader": "^7.1.0",
    "showdown": "^1.8.6",
    "style-loader": "^0.23.0",
    "webpack": "^4.19.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.4"
  }
}
